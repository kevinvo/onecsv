{
  "name": "app",
  "private": "true",
  "dependencies": {
    "@popperjs/core": "^2.11.6",
    "@tanstack/react-table": "^8.5.13",
    "axios": "^0.21.1",
    "bootstrap": "^5.2.1",
    "bootstrap-icons": "^1.9.1",
    "esbuild": "^0.15.7",
    "esbuild-wasm": "^0.15.7",
    "eslint": "^8.23.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.5.0",
    "react-data-grid": "^7.0.0-beta.15",
    "react-debounce-input": "^3.3.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.0.0",
    "react-table": "^7.8.0",
    "sass": "^1.54.5",
    "styled-components": "^5.3.5",
    "xlsx": "^0.18.5"
  },
  "scripts": {
    "build": "esbuild app/typescript/*.* --bundle --sourcemap --outdir=app/assets/builds --public-path=assets",
    "build:css": "sass ./app/assets/stylesheets/application.bootstrap.scss:./app/assets/builds/application.css --no-source-map --load-path=node_modules",
    "lint": "eslint app/typescript/**/*.{ts,tsx}",
    "lint:fix": "eslint --fix 'app/typescript/**/*.{ts,tsx}'",
    "format": "prettier --write 'app/typescript/**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.7.1",
    "typescript": "^4.8.3"
  }
}
